<%# ユーザー一覧ページ #%>

<p id="notice"><%= notice %></p>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'ユーザーの情報を見る', user %></td>
        <td><%= link_to '編集する', edit_user_path(user) %></td>
        <td><%= link_to '削除する', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>

<br>

